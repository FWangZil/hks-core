    "/user/get": {
      "get": {
        "summary": "获取单个用户信息",
        "operationId": "getUserByQuery",
        "produces": ["application/json"],
        "tags": ["user"],
        "responses": {
          "200": {
            "description": "成功",
            "schema": {
              "type": "object"
            }
          }
        },
        "parameters": [
          {
            "name": "token",
            "in": "header",
            "description": "token",
            "required": false,
            "type": "string"
          },
          {
            "name": "userID",
            "in": "query",
            "description": "用户ID",
            "type": "integer"
          },
          {
            "name": "mobile",
            "in": "query",
            "description": "用户手机号码",
            "type": "string"
          }
        ]
      }
    },
    "/user/list": {
      "get": {
        "summary": "获取用户列表信息",
        "operationId": "listUser",
        "produces": ["application/json"],
        "tags": ["user"],
        "responses": {
          "200": {
            "description": "成功",
            "schema": {
              "type": "object"
            }
          }
        },
        "parameters": [
          {
            "name": "token",
            "in": "header",
            "description": "token",
            "required": false,
            "type": "string"
          },
          {
            "name": "type",
            "in": "query",
            "description": "用户类型",
            "type": "integer"
          },
          {
            "name": "gender",
            "in": "query",
            "description": "用户性别",
            "type": "integer"
          },
          {
            "name": "pageSize",
            "in": "query",
            "description": "页数",
            "type": "integer"
          },
          {
            "name": "page",
            "in": "query",
            "description": "页码",
            "type": "integer"
          }
        ]
      }
    },
    "/user/register": {
      "post": {
        "summary": "用户注册借口",
        "operationId": "registerUser",
        "produces": ["application/json"],
        "tags": ["user"],
        "responses": {
          "200": {
            "description": "成功",
            "schema": {
              "type": "object"
